{
  "protocol": "https",
  "port": 443,
  "host": "localhost",
  "maxTotal": 100,
  "maxPerRoute": 20,
  "SoTimeout": 60,
  "https": {
    "keyStoreFile": "localhost.p12",
    "keyPassword": "changeit",
    "keyStoreType": "PKCS12",
    "protocol": "TLSv1.2",
    "support-protocols": "TLSv1.2,TLSv1.3",
    "clientAuth": false,
    "caKeyStoreFile": "cacerts",
    "caKeyPassword": "changeit",
    "crl": "CRL.der"
  },
  "services": [
    {
      "path": "/",
      "type": "normal",
      "id": "default",
      "docsRoot": "src/test/resources/htdocs"
    },
    {
      "path": "/examples/",
      "type": "reverse",
      "id": "examples",
      "reverse": {
        "url": "http://localhost:8080/examples/"
      }
    }
  ]
}